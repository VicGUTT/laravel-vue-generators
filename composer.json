{
    "name": "zachleigh/laravel-vue-generators",
    "description": "Generate Vue js files via artisan commands.",
    "keywords" : ["laravel", "vue", "generators"],
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Zach Leigh",
            "email": "zachleigh@fastmail.jp"
        }
    ],
    
    "require": {
        "php": ">=5.6.0"
    },

    "require-dev": {
        "laravel/laravel": "5.3.*",
        "phpunit/phpunit": "~4.0"
    },

    "autoload": {
        "psr-4": {
            "VueGenerators\\": "src/"
        }
    },

    "autoload-dev": {
        "psr-4": {
            "VueGenerators\\tests\\": "tests/"
        }
    },

    "scripts": {
        "test": "phpunit"
    },

    "minimum-stability": "stable",
    "prefer-stable": true
}
